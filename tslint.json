{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    // enable tslint-react rules here
    "jsx-self-close": true,
    "quotemark": [true, "single", "avoid-escape"],
    "jsx-no-lambda": false,
    "variable-name": ["allow-leading-underscore"],
    "max-line-length": [true, 200],
    "jsx-alignment": false
  }
}